<!--
This XML defines document root factory, class, methods, noun index suffix, and action expression suffix
for a noun so that REST services can automatically create and execute BusinessObjectDocument.
-->

<ServiceClientLibrary>
    <Noun name="ClientLocation">
        <DocumentRootFactory name="com.ibm.commerce.location.facade.datatypes.LocationFactory"/>
        <ClientClass name="com.ibm.commerce.location.facade.client.LocationFacadeClient"/>
        <Method verb="Get" name="getClientLocation"/>
        <Method verb="Change" name="changeClientLocation"/>
		<Method verb="Process" name="processClientLocation"/>
		<ActionExpressionSuffixForNoun value=""/>
    </Noun>
    <Noun name="PointOfInterest">
        <DocumentRootFactory name="com.ibm.commerce.location.facade.datatypes.LocationFactory"/>
        <ClientClass name="com.ibm.commerce.location.facade.client.LocationFacadeClient"/>
        <Method verb="Get" name="getPointOfInterest"/>
        <Method verb="Change" name="changePointOfInterest"/>
		<Method verb="Process" name="processPointOfInterest"/>
		<ActionExpressionSuffixForNoun value=""/>
    </Noun>
</ServiceClientLibrary>