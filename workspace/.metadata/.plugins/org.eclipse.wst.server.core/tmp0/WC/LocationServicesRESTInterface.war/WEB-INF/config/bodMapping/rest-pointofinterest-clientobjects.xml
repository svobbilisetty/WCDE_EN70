<_config:URLtoOAGIS
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/url-to-oagis.xsd " xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">
	
	<_config:NounDefinitions>	
		<_config:Noun name="PointOfInterest"/>
	</_config:NounDefinitions>
	
	<_config:URLDefinitions>
	
		<_config:URLParameterGroup name="pointOfInterest" noun="PointOfInterest">
			<_config:URLParameter name="pointOfInterestId" nounElement="/PointOfInterestIdentifier/UniqueID" key="true" return="true" />
			<_config:URLParameter name="programId" nounElement="/PointOfInterestIdentifier/ExternalIdentifier/ProgramID" key="false" return="true" />
			<_config:URLParameter name="storeId" nounElement="/PointOfInterestIdentifier/ExternalIdentifier/StoreID" key="false" return="true" />
			<_config:URLParameter name="identifier" nounElement="/PointOfInterestIdentifier/ExternalIdentifier/Identifier" key="false" return="true" />
			<_config:URLParameter name="status" nounElement="/Status" key="false" return="true" />
			<_config:URLParameter name="poiType" nounElement="/POIType" key="false" return="true" />
			<_config:URLParameter name="regionId" nounElement="/RegionID" key="false" return="true" />
			<_config:URLParameter name="zoneId" nounElement="/ZoneID" key="false" return="true" />
			<_config:URLParameter name="cellId" nounElement="/CellID" key="false" return="true" />
			<_config:URLParameter name="pointId" nounElement="/PointID" key="false" return="true" />
			<_config:URLParameter name="description/langId" nounElement="/Description/@language" key="false" return="true" />
			<_config:URLParameter name="description/name" nounElement="/Description/Name" key="false" return="true" />
			<_config:URLParameter name="description/shortDesc" nounElement="/Description/ShortDescription" key="false" return="true" />
			<_config:URLParameter name="description/longDesc" nounElement="/Description/LongDescription" key="false" return="true" />
			<_config:URLParameter name="description/image1" nounElement="/Description/DisplayImage1" key="false" return="true" />
			<_config:URLParameter name="description/image2" nounElement="/Description/DisplayImage2" key="false" return="true" />
			<_config:URLParameter name="description/xdesc_" nounElement="/Description/UserData/UserDataField" key="false" return="true" type="UserData"/> 
			<_config:URLParameter name="tags" nounElement="/Tags" key="false" return="true" />
			<_config:URLParameter name="latitude" nounElement="/Latitude" key="false" return="true" />
			<_config:URLParameter name="longitude" nounElement="/Longitude" key="false" return="true" />
			<_config:URLParameter name="altitude" nounElement="/Altitude" key="false" return="true" />
			<_config:URLParameter name="radius" nounElement="/Radius" key="false" return="true" />
			<_config:URLParameter name="precedence" nounElement="/Precedence" key="false" return="true" />
			<_config:URLParameter name="x_" nounElement="/UserData/UserDataField" key="false" return="true" type="UserData" />
		</_config:URLParameterGroup>
        
	</_config:URLDefinitions>

</_config:URLtoOAGIS>

