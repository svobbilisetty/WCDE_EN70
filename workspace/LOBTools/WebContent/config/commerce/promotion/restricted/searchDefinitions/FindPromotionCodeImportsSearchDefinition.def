<Definitions>
	<!---
		This search is used to find {@link PromotionCodeImport PromotionCodeImport}. The search retrieves objects that
		match the search term. The search term text is compared with the values of the following property:
		<ul>
			<li>fileName - this is the name of the promotion code import task.</li>			
		</ul>
	-->
	<SearchDefinition definitionName="FindPromotionCodeImports" displayName="${promotionResources.promotionCodeImportObjectName}" headerText="${promotionResources.promotionCodeImportList_fileName}" helpLink="tasks/ttfsearch.htm" icon="promotionCodeImportIcon" listDefinitionName="proPromotionCodeImportGrid" listTitle="${promotionResources.promotionCodeImportSearchResultTitle}" searchType="FindPromotionCodeImports" tooltip="${promotionResources.searchPromotionCodeImportsTooltip}">
			<SearchService name="findPromotionCodeImports" url="/cmc/FindPromotionCodeImports">
				<ServiceParam name="storeId"/>				
			</SearchService>
	</SearchDefinition>	
</Definitions>