<Definitions>

	<!---
		@keywords private
	-->
	<ObjectGrid definitionName="wspApproverGrid" preferenceKey="wspApproverGrid">
		<GridText editable="false" name="memberId" objectPath="Approver" propertyName="memberId" text="${wspWorkspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" objectPath="Approver" propertyName="distinguishedName" text="${wspWorkspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" objectPath="Approver" propertyName="logonId" required="true" text="${wspWorkspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" objectPath="Approver" propertyName="firstName" text="${wspWorkspaceResources.personFirstName}" width="120"/>
		<GridText editable="false" name="lastName" objectPath="Approver" propertyName="lastName" text="${wspWorkspaceResources.personLastName}" width="120"/>
	</ObjectGrid>
	
	<ObjectGrid definitionName="wspContributorGrid" preferenceKey="wspContributorGrid">
		<GridText editable="false" name="memberId" objectPath="Contributor" propertyName="memberId" text="${wspWorkspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" objectPath="Contributor" propertyName="distinguishedName" text="${wspWorkspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" objectPath="Contributor" propertyName="logonId" required="true" text="${wspWorkspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" objectPath="Contributor" propertyName="firstName" text="${wspWorkspaceResources.personFirstName}" width="120"/>
		<GridText editable="false" name="lastName" objectPath="Contributor" propertyName="lastName" text="${wspWorkspaceResources.personLastName}" width="120"/>
	</ObjectGrid>
	
	<ObjectGrid definitionName="wspPersonSearchGrid" preferenceKey="wspPersonSearchGrid">
		<wcfGridIconTypeImage name="typeIcon" text="${wspWorkspaceResources.type}" width="100" propertyName="null"/>	
		<GridText editable="false" name="memberId" propertyName="memberId" text="${wspWorkspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" propertyName="distinguishedName" text="${wspWorkspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" propertyName="logonId" required="true" text="${wspWorkspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" propertyName="firstName" text="${wspWorkspaceResources.personFirstName}" width="120" visible="false"/>
		<GridText editable="false" name="lastName" propertyName="lastName" text="${wspWorkspaceResources.personLastName}" width="120" visible="false"/>
	</ObjectGrid>

</Definitions>