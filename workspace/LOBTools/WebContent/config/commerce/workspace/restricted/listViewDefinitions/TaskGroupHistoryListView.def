<Definitions>

	<!---
		@keywords private
		This class displays the contents and characteristics of a list of {@link wspTaskGroupComments TaskGroupComment}.
	-->
	<ObjectGrid definitionName="wspTaskGroupHistoryListView" preferenceKey="wspTaskGroupHistoryListView">
	      <GridText editable="false" name="taskGroupHistoryId" propertyName="taskGroupHistoryId" visible="false" text="${wspWorkspaceResources.taskGroupHistoryId}"/>
          <GridPropertyImage name="objectType" propertyName="objectType" editable="false" enableFilter="true" text="${wspWorkspaceResources.type}" width="80" visible="true"/>		  
		  <GridText editable="false" name="objectCode" propertyName="objectCode" required="false" text="${wspWorkspaceResources.objectCode}" width="120"/>
		  <GridStoreName name="store" propertyName="objectStoreId" width="125" alwaysVisible="true"/>
		  <GridText editable="false" name="task" propertyName="task" required="false" enableFilter="true" text="${wspWorkspaceResources.lastTask}" width="120"/>
		  <GridComboBox editable="false" name="action" propertyName="action" required="false" enableFilter="true" text="${wspWorkspaceResources.action}" width="120"/>
		  <GridText editable="false" name="modifiedBy" propertyName="modifiedBy" required="false" enableFilter="true" text="${wspWorkspaceResources.modifiedBy}" width="140"/>
		  <GridDateTime editable="false" name="modifiedTime" propertyName="modifiedTime" text="${wspWorkspaceResources.modifiedTime}" width="170"/>  		  
	</ObjectGrid>

</Definitions>