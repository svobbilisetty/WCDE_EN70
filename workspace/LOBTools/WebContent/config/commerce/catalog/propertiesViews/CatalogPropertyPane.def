		</PropertyGroup>
		
		<!-- Use this property group to display the keyword property when the description override feature is enabled.  -->
		<PropertyGroup collapsable="false" name="catOverrideKeywordGroup">
		<EnablementAndCondition baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions"/>
			<!-- Property: Keyword. A multiline input box for the search optimization keyword property. -->
			<PropertyInputMultiLineText extendedHelpText="${catalogResources.extendedHelpText_CatalogEntryKeyword}" objectPath="CatalogEntryDescription" promptText="${catalogResources.keywordPrompt}" propertyName="keyword" displayObjectStoreName="true"/>
			<!-- Property: Keyword Override. A multiline input box for the search optimization keyword override property. -->
			<PropertyInputMultiLineText extendedHelpText="${catalogResources.extendedHelpText_CatalogEntryKeyword}" objectPath="CatalogEntryDescriptionOverride" promptText="${catalogResources.keywordPrompt}" propertyName="overrideKeyword" displayStoreName="true"/>
		</PropertyGroup>
		
        <!-- Property: Additional Description 1. A multiline input box for the auxilary description 1 property. -->
        <!-- This field is disabled by default. Uncomment this section to enable the field -->
        <!-- <PropertyRichTextEditor
            objectPath="CatalogEntryDescription"
            promptText="${catalogResources.addDescription1Prompt}" propertyName="xdesc_auxDescription1"/> -->
        <!-- Property: Additional Description 2. A multiline input box for the auxilary description 2 property. -->
        <!-- This field is disabled by default. Uncomment this section to enable the field -->
        <!-- <PropertyRichTextEditor
            objectPath="CatalogEntryDescription"
            promptText="${catalogResources.addDescription2Prompt}" propertyName="xdesc_auxDescription2"/> -->
        <!-- Property: Manufacturer Name. An input box for the manufacturer name property. -->
        <PropertyInputText objectPath="CatalogEntryExtraProperties" promptText="${catalogResources.manufacturerPrompt}" propertyName="xprop_manufacturer"/>
        <!-- Property: Manufacturer Partnumber. An input box for the manufacturer part number property. -->
        <PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_manufacturerPartNumber}" objectPath="CatalogEntryExtraProperties" promptText="${catalogResources.manufacturerPartNumberPrompt}" propertyName="xprop_manufacturerPartNumber"/>
        <!-- Property: URL. An input box for the URL of the catalog entry property. -->    
        <PropertyInputLongText extendedHelpText="${catalogResources.extendedHelpText_url}" objectPath="CatalogEntryExtraProperties" promptText="${catalogResources.urlPrompt}" propertyName="xprop_url"/>
        <!--- An editor to manage the parent category of a product -->
        <ReferenceEditor allowCreate="true" extendedHelpText="${catalogResources.extendedHelpText_catalogEntryCategory}" headerText="${catalogResources.codePrompt}" name="parentCategoryRefEditor" parentObjectTypes="CatalogGroup,InheritedCatalogGroup" promptText="${catalogResources.parentCategoryInMasterCatalogPrompt}" referenceObjectTypes="ChildProduct,ChildSalesCatalogEntry,ChildSalesProduct,ChildInheritedProduct,ChildSalesInheritedProduct,ChildSalesInheritedCatalogEntry" required="true" validatorTypes="requiredParentCategoryValidator,requiredParentVersionedCategoryValidator"/>
        <!-- Property: Recurring order. A radio group for the disallow recurring order property. -->
	<MultiValueSelector name="recurringOrderSelector" objectPath="CatalogEntryExtraProperties" promptText="${catalogResources.catalogEntryRecurringOrder}" useRadioButtons="true" helpText="${catalogResources.extendedHelpText_catalogEntryRecurringOrder}">
		<MultiValueSelection displayName="${catalogResources.catalogEntryRecurringOrderTrue}" name="recurringOrderTrue">
			<Xml name="values">
				<xprop_disallowRecurringOrder>0</xprop_disallowRecurringOrder>
				<xprop_subscriptionTypeId significant="false">NONE</xprop_subscriptionTypeId>
			</Xml>
		</MultiValueSelection>
		<MultiValueSelection displayName="${catalogResources.catalogEntryRecurringOrderFalse}" name="recurringOrderFalse">
			<Xml name="values">
				<xprop_disallowRecurringOrder>1</xprop_disallowRecurringOrder>
			</Xml>
		</MultiValueSelection>
	</MultiValueSelector>  
        <!-- Property: Subscription Type Id. A radio group for the subscription type id property. -->
	<MultiValueSelector name="subscriptionSelector" objectPath="CatalogEntryExtraProperties" promptText="${catalogResources.catalogEntrySubscription}" useRadioButtons="true" helpText="${catalogResources.extendedHelpText_catalogEntrySubscription}">
		<MultiValueSelection displayName="${catalogResources.catalogEntrySubscriptionTrue}" name="catalogEntrySubscriptionTrue">
			<Xml name="values">
				<xprop_subscriptionTypeId>TIME-BASED</xprop_subscriptionTypeId>
				<xprop_disallowRecurringOrder significant="false">1</xprop_disallowRecurringOrder>
			</Xml>
		</MultiValueSelection>
		<MultiValueSelection displayName="${catalogResources.catalogEntrySubscriptionFalse}" name="catalogEntrySubscriptionFalse">
			<Xml name="values">
				<xprop_subscriptionTypeId>NONE</xprop_subscriptionTypeId>
			</Xml>
		</MultiValueSelection>
	</MultiValueSelector>  
    </PropertyGroup>

