<Definitions>


    <PropertyGroup definitionName="catManageGeneralProductInformation" groupTitle="${catalogResources.generalProductInformationSection}" open="true">
        <!-- Catalog Entry Unique ID -->
        <PropertyViewLongText promptText="${catalogResources.uniqueId_ColumnHeader}" propertyName="catentryId">
             <FalseEnablementCondition/>
        </PropertyViewLongText>
        <!-- Property: Code. An input box for the code property. -->
        <PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_Code}" promptText="${catalogResources.codePrompt}" propertyName="partnumber" required="true"/>
		
		<!-- Use this property group to display the name property when the description override feature is disabled.  -->
		<PropertyGroup collapsable="false" name="catNameGroup">
			<!-- Property: Name. An input box for the name property. -->
			<PropertyInputText objectPath="CatalogEntryDescription" promptText="${catalogResources.namePrompt}" propertyName="name"/>
			<EnablementAndCondition conditionId="inheritedCatalogEntryCondition" baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions" negate="true"/>
		</PropertyGroup>
		
		<!-- Use this property group to display the name property when the description override feature is enabled.  -->
		<PropertyGroup collapsable="false" name="catOverrideNameGroup">
			<EnablementAndCondition conditionId="inheritedCatalogEntryCondition" baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions"/>
			<!-- Property: Name. An input box for the name property. -->
			<PropertyInputText objectPath="CatalogEntryDescription" promptText="${catalogResources.namePrompt}" propertyName="name" displayObjectStoreName="true"/>
			<!-- Property: Name Override. An input box for the name override property. -->
			<PropertyInputText objectPath="CatalogEntryDescriptionOverride" promptText="${catalogResources.namePrompt}" propertyName="overrideName" displayStoreName="true"/>
		</PropertyGroup>
		
		<!-- Use this property group to display the short description property when the description override feature is disabled.  -->
		<PropertyGroup collapsable="false" name="catSDescGroup">
			<EnablementAndCondition conditionId="inheritedCatalogEntryCondition" baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions" negate="true"/>
			<!-- Property: Short Description. A long input box for the short description property. -->
			<PropertyInputLongText objectPath="CatalogEntryDescription" promptText="${catalogResources.shortDescriptionPrompt}" propertyName="sDesc"/>
		</PropertyGroup>
		
		<!-- Use this property group to display the short description property when the description override feature is enabled.  -->
		<PropertyGroup collapsable="false" name="catOverrideSDescGroup">
			<EnablementAndCondition baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions"/>
			<!-- Property: Short Description. A long input box for the short description property. -->
			<PropertyInputLongText objectPath="CatalogEntryDescription" promptText="${catalogResources.shortDescriptionPrompt}" propertyName="sDesc" displayObjectStoreName="true"/>
			<!-- Property: Short Description Override. A long input box for the short description override property. -->
			<PropertyInputLongText objectPath="CatalogEntryDescriptionOverride" promptText="${catalogResources.shortDescriptionPrompt}" propertyName="overrideSDesc" displayStoreName="true"/>
		</PropertyGroup>
		
		<!-- Use this property group to display the long description property when the description override feature is disabled.  -->
