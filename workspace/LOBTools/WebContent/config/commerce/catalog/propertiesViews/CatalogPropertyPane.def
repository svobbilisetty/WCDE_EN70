	<PropertyPane definitionName="catSearchEngineOptimization">
		<!-- Property Group: Contains the catalog entry SEO properties inherited from the catalog asset store -->
		<PropertyGroup name="catCatEntrySeoAssetStoreSection" baseDefinitionName="catManageInheritedCatalogEntrySeoAttributes"/>
		<!-- Property Group: Contains the catalog entry SEO properties of the local store (shows store name in the group title) -->
		<PropertyGroup name="catCatEntrySeoESiteStoreSection" baseDefinitionName="catManageCatalogEntrySeoAttributes" open="true">
			<EnablementAndCondition conditionId="inheritedObjectTypesAndStoreCondition">
					<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
					<EnablementOrCondition conditionId="storeTypeCondition" negate="true">
						<EnablementCondition conditionId="catAssetStoreCondition" contextName="storeType" enablementValue="CPS"/>
						<EnablementCondition conditionId="b2cStoreCondition" contextName="storeType" enablementValue="B2C"/>
						<EnablementCondition conditionId="b2bStoreCondition" contextName="storeType" enablementValue="B2B"/>
					</EnablementOrCondition>
			</EnablementAndCondition>
		</PropertyGroup>
		<!-- Property Group: Contains the catalog entry SEO properties of the local store (no group title) -->
		<PropertyGroup name="catCatEntrySeoLocalStoreSection" baseDefinitionName="catManageCatalogEntrySeoAttributes" displayObjectStoreName="false" displayStoreName="false" collapsable="false">
			<EnablementAndCondition conditionId="localObjectTypesAndStoreCondition">
					<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>
					<EnablementOrCondition conditionId="storeTypeCondition">
						<EnablementCondition conditionId="catAssetStoreCondition" contextName="storeType" enablementValue="CPS"/>
						<EnablementCondition conditionId="b2cStoreCondition" contextName="storeType" enablementValue="B2C"/>
						<EnablementCondition conditionId="b2bStoreCondition" contextName="storeType" enablementValue="B2B"/>
						<EnablementCondition conditionId="b2cESiteStoreCondition" contextName="storeType" enablementValue="MHS"/>
						<EnablementCondition conditionId="b2bESiteStoreCondition" contextName="storeType" enablementValue="BMH"/>
					</EnablementOrCondition>
			</EnablementAndCondition>
		</PropertyGroup>
	</PropertyPane>
	
	<PropertyPane definitionName="catCatalogGroupSearchEngineOptimization">
		<!-- Property Group: Contains the catalog group SEO properties inherited from the catalog asset store -->
		<PropertyGroup name="catCatGroupSeoAssetStoreSection" baseDefinitionName="catManageInheritedCatalogGroupSeoAttributes"/>
		<!-- Property Group: Contains the catalog group SEO properties of the local store (shows store name in the group title) -->
		<PropertyGroup name="catCatGroupSeoESiteStoreSection" baseDefinitionName="catManageCatalogGroupSeoAttributes" open="true">
			<EnablementAndCondition conditionId="inheritedObjectTypesAndStoreCondition">
					<EnablementOrCondition conditionId="inheritedObjectTypesCondition">
						<EnablementCondition checkObjectDefinition="true" conditionId="inheritedCatalogGroupCondition" enablementValue="InheritedCatalogGroup" propertyName="objectType"/>
						<EnablementCondition checkObjectDefinition="true" conditionId="inheritedSalesCatalogGroupCondition" enablementValue="InheritedSalesCatalogGroup" propertyName="objectType"/>
					</EnablementOrCondition>
					<EnablementOrCondition conditionId="storeTypeCondition" negate="true">
						<EnablementCondition conditionId="catAssetStoreCondition" contextName="storeType" enablementValue="CPS"/>
						<EnablementCondition conditionId="b2cStoreCondition" contextName="storeType" enablementValue="B2C"/>
						<EnablementCondition conditionId="b2bStoreCondition" contextName="storeType" enablementValue="B2B"/>
					</EnablementOrCondition>
			</EnablementAndCondition>
		</PropertyGroup>
		<!-- Property Group: Contains the catalog group SEO properties of the local store (no group title) -->
		<PropertyGroup name="catCatGroupSeoLocalStoreSection" baseDefinitionName="catManageCatalogGroupSeoAttributes" displayObjectStoreName="false" displayStoreName="false" collapsable="false">
			<EnablementAndCondition conditionId="localObjectTypesAndStoreCondition">
					<EnablementOrCondition conditionId="localObjectTypesCondition">
						<EnablementCondition checkObjectDefinition="true" conditionId="catalogGroupCondition" enablementValue="CatalogGroup" propertyName="objectType"/>
						<EnablementCondition checkObjectDefinition="true" conditionId="salesCatalogGroupCondition" enablementValue="SalesCatalogGroup" propertyName="objectType"/>
					</EnablementOrCondition>
					<EnablementOrCondition conditionId="storeTypeCondition">
						<EnablementCondition conditionId="catAssetStoreCondition" contextName="storeType" enablementValue="CPS"/>
						<EnablementCondition conditionId="b2cStoreCondition" contextName="storeType" enablementValue="B2C"/>
						<EnablementCondition conditionId="b2bStoreCondition" contextName="storeType" enablementValue="B2B"/>
						<EnablementCondition conditionId="b2cESiteStoreCondition" contextName="storeType" enablementValue="MHS"/>
						<EnablementCondition conditionId="b2bESiteStoreCondition" contextName="storeType" enablementValue="BMH"/>
					</EnablementOrCondition>
			</EnablementAndCondition>
		</PropertyGroup>
	</PropertyPane>
	
    <PropertyPane definitionName="catCatalogEntryDescriptiveAttributes">       
        <!-- 
        	Property Group: A properties group to display non-dictionary descriptive attributes for a catalog entry.

        	ATTRIBUTE DICTIONARY CUSTOMIZATION POINT: comment out the property group below if creating
        	non-dictionary attributes is desired.
        -->
		<PropertyGroup collapsable="false" name="catCatalogEntryDescriptiveAttributesPropertyGroup">
			<!-- 
				Enable if the product has classic descriptive attributes or
				the store does not use an attribute dictionary.
			 -->		
			<EnablementOrCondition>
				<EnablementCondition conditionId="StoreUsesDictionary" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled" negate="true"/>
				<ProductHasClassicDescriptiveAttributesEnablementCondition package="cat"/>
			</EnablementOrCondition>
			<!-- 
				An instantiation of a definition which contains a Child List Editor to display non-dictionary descriptive attributes. 
			-->
			<PropertyChildListEditor baseDefinitionName="catCatalogEntryDescriptiveAttributeList"/>
		</PropertyGroup>
        <!-- 
        	Property Group: A properties group to display dictionary descriptive attributes for a product.
        -->
		<PropertyGroup collapsable="false" name="catCatalogEntryDescriptiveDictionaryAttributesPropertyGroup">
			<!-- 
				Enable if the store uses an attribute dictionary.
			 -->	
			<EnablementCondition conditionId="attributeDictionaryIsEnabledCondition" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled"/>
			<!-- 
				An instantiation of a definition which contains a Child List Editor to display dictionary descriptive attributes. 
			-->
			<PropertyChildListEditor baseDefinitionName="catCatalogEntryAttrDictDescriptiveAttributeList"/>
			
		</PropertyGroup>
    </PropertyPane>
    
   <PropertyPane definitionName="catCatalogEntryDefiningAttributes">
  		<!-- 
  			Property Group: A properties group to display local defining attributes for a product. 
  			
        	ATTRIBUTE DICTIONARY CUSTOMIZATION POINT: comment out the property group below if viewing
        	non-dictionary attributes is not desired.	
  		-->
		<PropertyGroup collapsable="false" name="catCatalogEntryDefiningAttributesPropertyGroup">
			<!-- 
				Enable if the product has classic defining attributes and no AD defining attributes or
				the store does not use an attribute dictionary.
			 -->
			<EnablementOrCondition>
				<EnablementCondition conditionId="StoreUsesDictionary" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled" negate="true"/>
				<EnablementAndCondition>
					<EnablementCondition conditionId="catalogEntryHasAttributeDictionaryDefiningAttributesCondition" enablementValue="true" objectPath="CatalogEntryAttributeDictionaryDefiningAttributeProperties" propertyName="catalogEntryHasAttributeDictionaryDefiningAttributes" negate="true"/>
					<ProductHasClassicDefiningAttributesEnablementCondition package="cat"/>
				</EnablementAndCondition>
			</EnablementOrCondition>
			<!-- 
				An instantiation of a definition which contains a Child List Editor to display non-dictionary defining attributes. 
			-->
        	<PropertyChildListEditor baseDefinitionName="catCatalogEntryDefiningAttributeList"/>
		</PropertyGroup>    
        <!-- 
        	Property Group: A properties group to display attribute dictionary defining attributes for a product. 
        -->
		<PropertyGroup collapsable="false" name="catCatalogEntryDefiningDictionaryAttributesPropertyGroup">
			<!-- 
				Enable if the product has no classic defining attributes or has classic and AD defining attributes
				and the store uses an attribute dictionary.
			 -->
			<EnablementAndCondition>
				<EnablementCondition conditionId="StoreUsesDictionary" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled"/>
				<EnablementOrCondition>
					<EnablementAndCondition conditionId="1">
						<EnablementCondition conditionId="catalogEntryHasAttributeDictionaryDefiningAttributesCondition" enablementValue="true" objectPath="CatalogEntryAttributeDictionaryDefiningAttributeProperties" propertyName="catalogEntryHasAttributeDictionaryDefiningAttributes"/>
						<ProductHasClassicDefiningAttributesEnablementCondition package="cat"/>
					</EnablementAndCondition>
					<EnablementAndCondition conditionId="2">
						<ProductHasClassicDefiningAttributesEnablementCondition package="cat" negate="true"/>
					</EnablementAndCondition>
				</EnablementOrCondition>
			</EnablementAndCondition>
			<!-- 
				An instantiation of a definition which contains a Child List Editor to display local product defining attributes. 
			-->
			<PropertyChildListEditor baseDefinitionName="catCatalogEntryAttributeDictionaryDefiningAttributeList"/>
		</PropertyGroup>		
    </PropertyPane>
    
    <PropertyPane definitionName="catSKUAttributes">
        <!-- 
        	Property Group: A properties group to display non-dictionary descriptive attributes for a SKU.

        	ATTRIBUTE DICTIONARY CUSTOMIZATION POINT: comment out the property group below if creating
        	non-dictionary attributes is desired.
        -->
        <PropertyGroup collapsable="false" name="catCatalogEntryDescriptiveAttributesPropertyGroup">
        	<!-- 
				Enable if the product has classic descriptive attributes or
				the store does not use an attribute dictionary.
			 -->		
			<EnablementOrCondition>
				<EnablementCondition conditionId="StoreUsesDictionary" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled" negate="true"/>
				<ProductHasClassicDescriptiveAttributesEnablementCondition package="cat"/>
			</EnablementOrCondition>
			
			<PropertyChildListEditor baseDefinitionName="catCatalogEntryDescriptiveAttributeList"/>
		</PropertyGroup>
		
        <!-- 
        	Property Group: A properties group to display dictionary descriptive attributes for a product.
        -->
		<PropertyGroup collapsable="false" name="catCatalogEntryDescriptiveDictionaryAttributesPropertyGroup">
			<!-- 
				Enable if the store uses an attribute dictionary.
			 -->	
			<EnablementCondition conditionId="attributeDictionaryIsEnabledCondition" contextName="attributeDictionaryEnabledForStore" enablementValue="enabled"/>
			<!-- 
				An instantiation of a definition which contains a Child List Editor to display dictionary descriptive attributes. 
			-->
			<PropertyChildListEditor baseDefinitionName="catCatalogEntryAttrDictDescriptiveAttributeList"/>
		</PropertyGroup>
    </PropertyPane>

    <PropertyPane definitionName="catProductMerchandisingAssociations">
    
        <!-- An instantiation of a definition which will display merchandising associations in a grid. -->
		<PropertyGroup collapsable="false" name="catMerchandisingAssociationPropertyGroupListNoStoreName">
			<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>	
        	<PropertyChildListEditor baseDefinitionName="catMerchandisingAssociationListNoStoreName" clientActionTypes="LaunchCoremetricsMAReport"/>
		</PropertyGroup>
        <!-- An instantiation of a definition which will display inherited merchandising associations in a grid. -->
		<PropertyGroup collapsable="false" name="catInheritedMerchandisingAssociationPropertyGroupList">
			<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
        	<PropertyChildListEditor baseDefinitionName="catInheritedMerchandisingAssociationList" clientActionTypes="LaunchCoremetricsMAReport"/>
		</PropertyGroup>
	    <!-- An instantiation of a definition which will display inherited merchandising associations created locally in a grid. -->
		<PropertyGroup collapsable="false" name="catMerchandisingAssociationPropertyGroupListStoreName">
			<EnablementAndCondition conditionId="inheritedObjectTypesAndStoreCondition">
				<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
				<EnablementOrCondition conditionId="storeTypeCondition" negate="true">
					<EnablementCondition conditionId="demandHubCondition" contextName="storeType" enablementValue="CHS"/>
					<EnablementCondition conditionId="supplierHubCondition" contextName="storeType" enablementValue="SCP"/>
				</EnablementOrCondition>
			</EnablementAndCondition>
        	<PropertyChildListEditor baseDefinitionName="catMerchandisingAssociationListStoreName" clientActionTypes="LaunchCoremetricsMAReport"/>
		</PropertyGroup>
		
    </PropertyPane>
        
    <PropertyPane definitionName="catBundleComponents">
        <PropertyGroup collapsable="false" name="catBundleComponentChildList">
            <!-- An instantiation of a definition which will display the components in a bundle. -->
			<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>	
        	<PropertyChildListEditor baseDefinitionName="catBundleComponentChildList"/>
		</PropertyGroup>
        <!-- An instantiation of a definition which will display inherited components. -->
		<PropertyGroup collapsable="false" name="catInheritedBundleComponentChildList">
			<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
        	<PropertyChildListEditor baseDefinitionName="catInheritedBundleComponentChildList"/>
		</PropertyGroup>
    </PropertyPane>
    
    <PropertyPane definitionName="catKitComponents">
        <!-- Conditionally Visable Area: This area will change what is displayed depending on the value of the catenttypeId property. -->
        <PropertyArea name="kit">
            <!-- Enablement Condition: Only display this section if the catenttypeId property is equal to 'PackageBean' -->
            <EnablementCondition conditionId="packageBeanIdCondition" enablementValue="PackageBean" propertyName="catenttypeId"/>
            <!-- An instantiation of a definition which will display bundle components. -->
            <PropertyGroup collapsable="false" name="catKitComponentChildList">
	            <!-- An instantiation of a definition which will display the components in a bundle. -->
				<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>	
        		<PropertyChildListEditor baseDefinitionName="catKitComponentChildList"/>
			</PropertyGroup>
        	<!-- An instantiation of a definition which will display inherited components. -->
			<PropertyGroup collapsable="false" name="catInheritedKitComponentChildList">
				<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
        		<PropertyChildListEditor baseDefinitionName="catInheritedKitComponentChildList"/>
			</PropertyGroup>
        </PropertyArea>
        <!-- Conditionally Visable Area: This area will change what is displayed depending on the value of the catenttypeId property. -->
        <PropertyArea name="dynamicKit">
            <!-- Enablement Condition: Only display this section if the catenttypeId property is equal to 'DynamicKitBean' -->
            <EnablementCondition conditionId="dynamicKitBeanIdCondition" enablementValue="DynamicKitBean" propertyName="catenttypeId"/>
            <!-- An instantiation of a definition which will display bundle components. -->
            <PropertyGroup collapsable="false" name="catKitComponentChildList">
	            <!-- An instantiation of a definition which will display the components in a bundle. -->
				<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>	
        		<PropertyChildListEditor baseDefinitionName="catKitComponentChildList"/>
			</PropertyGroup>
        	<!-- An instantiation of a definition which will display inherited components. -->
			<PropertyGroup collapsable="false" name="catInheritedKitComponentChildList">
				<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
        		<PropertyChildListEditor baseDefinitionName="catInheritedKitComponentChildList"/>
			</PropertyGroup>
        </PropertyArea>
		<!-- Conditionally Visable Area: This area will change what is displayed depending on the value of the catenttypeId property. -->        
        <PropertyArea name="selectKitType">
            <!-- Enablement Condition: Only display this section if the catenttypeId property is equal to '' -->
            <EnablementCondition conditionId="blankIdCondition" enablementValue="" propertyName="catenttypeId"/>
			<!-- An instantiation of a definition which will display kit components. -->
            <PropertyGroup collapsable="false" name="catKitComponentChildList">
	            <!-- An instantiation of a definition which will display the components in a bundle. -->
				<EnablementOrCondition baseDefinitionName="catCatalogEntryEnablement"/>	
        		<PropertyChildListEditor baseDefinitionName="catKitComponentChildList"/>
			</PropertyGroup>
        	<!-- An instantiation of a definition which will display inherited components. -->
			<PropertyGroup collapsable="false" name="catInheritedKitComponentChildList">
				<EnablementOrCondition baseDefinitionName="catInheritedCatalogEntryEnablement"/>
        		<PropertyChildListEditor baseDefinitionName="catInheritedKitComponentChildList"/>
			</PropertyGroup>
	        </PropertyArea>
      </PropertyPane>
      
    <PropertyPane definitionName="catCatalogEntryInventoryPane">
        <!-- Property Group:  Inventory. This section displays properties relating to the inventory of a catentry. -->
        <PropertyGroup collapsable="false" groupTitle="${catalogResources.catalogEntryInventorySection}" name="inventoryPropGroup" open="true">
	        <PropertyArea name="trackInventoryArea">
	            <!-- Enablement Condition: Only display this section if the catenttypeId property is not equal to 'DynamicKitBean' -->
	            <EnablementCondition conditionId="dynamicKitBeanCondition" enablementValue="DynamicKitBean" negate="true" propertyName="catenttypeId"/>
	            <!-- Property: Track Inventory. A combobox to select how to track inventory of a catentry. Applied to both ATP and non-ATP store. -->
	            <PropertyCombobox objectPath="CatalogEntryInventory" promptText="${catalogResources.trackInventoryPrompt}" propertyName="xful_trackInventory"/>
	        </PropertyArea>
            <!-- Property: Allow Back Order. A checkbox for the allow back order property of a catentry. Only applied to ATP store. -->
            <PropertyCombobox extendedHelpText="${catalogResources.extendedHelpText_backOrderable}" objectPath="CatalogEntryInventory" promptText="${catalogResources.allowBackOrderPrompt}" propertyName="xful_backOrderable"/>
            <!-- Property: Force Back Order. A checkbox to select the force back order property. Only applied to ATP store. -->
            <PropertyCombobox extendedHelpText="${catalogResources.extendedHelpText_forceBackOrder}" objectPath="CatalogEntryInventory" promptText="${catalogResources.forceBackOrderPrompt}" propertyName="xful_forceBackOrder"/>
            <!-- Property: Returns Not Required For Refund. A checkbox to select the returns not required for refund property. Only applied to ATP store. -->
            <PropertyCombobox objectPath="CatalogEntryInventory" promptText="${catalogResources.returnsNotRequiredForRefundPrompt}" propertyName="xful_returnNotDesired"/>
            <!-- Property: Returns And Refunds Not Allowed. A checkbox to select the returns and refunds not allowed property. Only applied to ATP store. -->
            <PropertyCombobox objectPath="CatalogEntryInventory" promptText="${catalogResources.returnsAndRefundsNotAllowedPrompt}" propertyName="xful_creditable"/>
            <!-- Property: Fullfill Separately. A checkbox to select the fullfill separately property. Only applied to ATP store. -->
            <PropertyCombobox extendedHelpText="${catalogResources.extendedHelpText_releaseSeparately}" objectPath="CatalogEntryInventory" promptText="${catalogResources.fulfillSeparatelyPrompt}" propertyName="xful_releaseSeparately"/>
            <!-- Property: Smallest Measured Amount. An input box to select the smallest measured amount property. Only applied to ATP store. -->
            <PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_quantityMultiple}" objectPath="CatalogEntryInventory" promptText="${catalogResources.smallestMeasuredAmountPrompt}" propertyName="xful_quantityMultiple"/>
            <!-- Property: Unit Of Measure. A combobox to allow for the selection of the unit of measure property of a catentry. Applied to both ATP and non-ATP store. -->
            <PropertyCombobox objectPath="CatalogEntryInventory" promptText="${catalogResources.unitOfMeasurePrompt}" propertyName="xful_uom"/>
        </PropertyGroup>
    </PropertyPane>

    <PropertyPane definitionName="catBundleResources">
        <!-- Property Group: Bundle Reference. A properties group that displays any references to the catentry.  -->
        <PropertyGroup groupTitle="${catalogResources.merchandisingAssociationReference}" name="catBundleReferenceMerchandisingAssociations" open="true">
            
                <!-- Reference List: A table to display merchandising associations that this catentry is a part of. -->
                <PropertyReferenceList listDefinitionName="catBackReferenceAssociationGrid" name="merchandisingAssociationsRefList" promptText="${catalogResources.merchandisingAssociationsSection}" referenceObjectPaths="CatalogEntry/MerchandisingAssociationReferencedCatalogEntries,CatalogEntry/MerchandisingAssociationReferencedInheritedCatalogEntries,CatalogEntry/InheritedMerchandisingAssociationReferencedInheritedCatalogEntries"/>
                
        </PropertyGroup>
        
        <!-- Property Group: sales catalog group Reference. A properties group that displays any sales catalog group references to the catentry.  -->
				<PropertyGroup baseDefinitionName="catReferenceSalesCatalogGroup"/>
        
        <!-- Property Group: Activity Reference. A properties group that displays any marketing activity references to the bundle. -->
        <PropertyGroup groupTitle="${catalogResources.marketingActivityReferencesSection}" name="bundleReferenceActivities" open="false">
              <!-- Reference List: A table to display activities from which this bundle is referenced. -->
							<PropertyReferenceList listDefinitionName="catMarketingActivityReferenceGrid" name="catMarketingActivityBundleReferenceList" promptText="${catalogResources.marketingActivityReferences}" referenceObjectPaths="*/ChildCatentryGroup"/>                				
							<!-- do not display in catalog asset store because it does not refer to storefront asset store -->
							<EnablementCondition conditionId="casCondition" contextName="storeType" enablementValue="CPS" negate="true"/>
        </PropertyGroup>
        				        
    </PropertyPane>

	<PropertyGroup definitionName="catReferenceSalesCatalogGroup" groupTitle="${catalogResources.salesCatalogCategoryReference}" open="false">

		<!-- Reference List: A table to display sales catalog groups that this catalog entry is a child of. -->
		<PropertyReferenceList listDefinitionName="catSalesCatalogReferenceGrid" name="salesCatGroupRefList" promptText="${catalogResources.salesCatalogCategoryReferenceSection}" referenceObjectPaths="*/ChildSalesCatalogEntry,*/ChildSalesInheritedCatalogEntry"/>

	</PropertyGroup>

     <!-- 
	
		 catManageAttachment 
		 @Purpose: Attachment Reference Notebook: Attachment References of current business object
		 
		 This definition holds all of the properties that will be displayed on the Manage Attachment tab of the category or catentry properties notebook.
		 
	-->
	 <PropertyPane definitionName="catManageAttachment">
	    
	     <PropertyGroup collapsable="false" name="catAttachmentReferencePropertyGroup">	 
	         <EnablementOrCondition conditionId="objectTypesCondition">
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="productCondition" enablementValue="Product" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="bundleCondition" enablementValue="Bundle" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="kitCondition" enablementValue="Kit" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="catalogGroupCondition" enablementValue="CatalogGroup" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="catalogGroupSKUCondition" enablementValue="CatalogGroupSKU" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="productSKUCondition" enablementValue="ProductSKU" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="salesCatalogGroupCondition" enablementValue="SalesCatalogGroup" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="salesCatalogCondition" enablementValue="SalesCatalog" propertyName="objectType"/>
	          </EnablementOrCondition>
		      <PropertyChildListEditor baseDefinitionName="catAttachmentReferenceList" objectTypes="AttachmentReference,BusinessObjectToInheritedAttachment"/>
	     </PropertyGroup>
	     	
		 <PropertyGroup collapsable="false" name="catInheritedAttachmentReferencePropertyGroup">			
			 <EnablementOrCondition conditionId="inheritedObjectTypesCondition">
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedProductCondition" enablementValue="InheritedProduct" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedBundleCondition" enablementValue="InheritedBundle" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedKitCondition" enablementValue="InheritedKit" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedCatalogGroupCondition" enablementValue="InheritedCatalogGroup" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedCatalogGroupSKUCondition" enablementValue="InheritedCatalogGroupSKU" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedSalesCatalogGroupCondition" enablementValue="InheritedSalesCatalogGroup" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedSalesCatalogCondition" enablementValue="InheritedSalesCatalog" propertyName="objectType"/>
	          	 <EnablementCondition checkObjectDefinition="true" conditionId="inheritedProductSKUCondition" enablementValue="InheritedProductSKU" propertyName="objectType"/>	          	
	         </EnablementOrCondition>
			 <PropertyChildListEditor baseDefinitionName="catAttachmentReferenceList" objectTypes="InheritedBusinessObjectToInheritedAttachment"/>
		</PropertyGroup>
	 </PropertyPane>
</Definitions>
