		</PropertyGroup>
		
		<!-- Use this property group to display the keyword property when the description override feature is disabled.  -->
		<PropertyGroup collapsable="false" name="catKeywordGroup">
			<EnablementAndCondition conditionId="inheritedCatalogEntryCondition" baseDefinitionName="catInheritedCatalogEntryAndOverrideEnabledConditions" negate="true"/>
