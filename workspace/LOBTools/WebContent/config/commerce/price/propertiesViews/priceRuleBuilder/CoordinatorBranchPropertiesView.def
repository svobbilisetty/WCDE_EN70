<Definitions>
	<!---
		Properties view for the {@link prcCoordinatorBranchElementObject}.
	-->
	<FlowElementProperties definitionName="prcCoordinatorBranchElementProperties">

		<PropertyGroup collapsable="false" name="coordinatorBranch">
			<PropertyInputText name="coordinatorName" propertyName="name" promptText="${prcPriceResources.nameString}" helpText="${prcPriceResources.helpText_branchName}"/>
			<PropertyRadioGroup promptText="${prcPriceResources.branchType}" propertyName="coordinatorType" helpText="${prcPriceResources.helpText_coordinatorType}" helpLinkURL="concepts/cpibranchcoor.htm"/>
			<PropertyPathListEditor listDefinitionName="prcBranchPathsGrid" name="pathEditor" objectTypes="path" promptText="${prcPriceResources.branchPaths}" helpText="${prcPriceResources.helpText_paths}"/>
		</PropertyGroup>
					
	</FlowElementProperties>

</Definitions>