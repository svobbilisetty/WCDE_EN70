<Definitions>
	<ObjectGrid definitionName="prcPriceRuleRefGrid" preferenceKey="prcPriceRuleRefGrid">
        <GridText editable="false" name="priceRuleId" propertyName="priceRuleId" text="${prcPriceResources.uniqueId}" visible="false" width="90"/>
    	<GridIconTypeImage name="typeIcon" propertyName="" required="true" text="${prcPriceResources.type}" width="100"/>
    	<GridStoreName name="store" propertyName="objectStoreId" text="${prcPriceResources.store}" width="125"/>
    	<GridText editable="false" name="priceRuleName"  propertyName="name" text="${prcPriceResources.nameString}" width="175"/>
    	<GridText editable="false" name="priceRuleDescription"  propertyName="description" text="${prcPriceResources.description}" width="250"/>
    </ObjectGrid> 
   
   <ObjectGrid definitionName="prcContractRefGrid" preferenceKey="prcContractRefGrid">
        <GridText editable="false" name="contractId" propertyName="contractId" text="${prcPriceResources.uniqueId}" visible="false" width="90"/>
    	<GridText editable="false" name="contractName"  propertyName="name" text="${prcPriceResources.nameString}" width="175"/>
    	<GridText editable="false" name="contractDescription"  propertyName="description" text="${prcPriceResources.description}" width="250"/>
    </ObjectGrid> 
    
    <ObjectGrid definitionName="prcPriceEquationRefGrid" preferenceKey="prcPriceEquationRefGrid">
    	<GridIconTypeImage name="typeIcon" propertyName="" required="true" text="${prcPriceResources.type}" width="100"/>
    	<GridText editable="false" name="nameString" propertyName="name" text="${prcPriceResources.nameString}" width="160"/>
    	<GridStoreName name="store" propertyName="objectStoreId" text="${prcPriceResources.store}" width="125"/>
    	<GridText editable="false" name="description" propertyName="description" text="${prcPriceResources.description}" width="300"/>
    	<GridText alignment="right" datatype="number" editable="false" name="uniqueID" propertyName="priceEquationId" text="${prcPriceResources.uniqueId}" visible="false" width="90"/>
    </ObjectGrid> 
    
</Definitions>

