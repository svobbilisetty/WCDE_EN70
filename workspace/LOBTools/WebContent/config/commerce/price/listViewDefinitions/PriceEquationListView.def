	<NavigationListDefinition definitionName="prcPriceEquationGridChildList" displayName="${prcPriceResources.priceEquationList}" helpLink="" isDefault="true" listDefinitionName="prcPriceEquationListEditor" listTitle="${prcPriceResources.list}" toolbarIcon="listToolbarIcon"/>
		
