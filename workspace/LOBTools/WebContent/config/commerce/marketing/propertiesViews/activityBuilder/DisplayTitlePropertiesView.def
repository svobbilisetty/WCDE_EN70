<Definitions>
	<!---
		Properties view for the {@link mktDisplayTitleFlowElementObject Display title action}.
	-->	
	<FlowElementProperties definitionName="mktDisplayTitlePropertiesView">
		<PropertyGroup collapsable="false" name="mktTitleContentGridPropertyGroup">
			<PropertyChildListEditor
				allowCreateReferencedPrimary="true"
				sequencePropertyName="sequence"
				listDefinitionName="mktContentGrid"
				name="TitleContent"
				objectTypes="ChildMarketingContent,ChildInheritedMarketingContent"
				promptText="${mktMarketingResources.titleContent}"
				required="true"
				validatorTypes="noMarketingContentSelected"/>
		</PropertyGroup>
	</FlowElementProperties>
	
</Definitions>