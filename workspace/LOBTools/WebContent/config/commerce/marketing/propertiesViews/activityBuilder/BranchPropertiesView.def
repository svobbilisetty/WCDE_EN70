<Definitions>
	<!---
		Properties view for the {@link mktBranchElementObject Branch element}.
	-->
	<FlowElementProperties definitionName="mktBranchProperties">
		<PropertyGroup collapsable="false" name="group">
			<PropertyInputText helpText="${mktMarketingResources.help_branch_name}" name="objName" promptText="${mktMarketingResources.objectName}" propertyName="name"/>
			<PropertyCombobox helpLinkURL="concepts/csbbranches.htm" helpText="${mktMarketingResources.help_branch_branchType}" name="branchtype" promptText="${mktMarketingResources.branchType}" propertyName="branchtype"/>
			
			<PropertyPathListEditor package="mkt" helpText="${mktMarketingResources.help_branch_paths}" listDefinitionName="mktExperimentPathGrid" name="pathEditor" objectTypes="path" promptText="${mktMarketingResources.paths}"/>
		</PropertyGroup>
	</FlowElementProperties>
