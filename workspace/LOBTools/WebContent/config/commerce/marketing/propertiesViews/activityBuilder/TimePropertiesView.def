<Definitions>
	<!---
		Properties view for the {@link mktTimeElementObject Day and Time target}.
	-->
	<FlowElementProperties definitionName="mktTimeProperties">
		<PropertyGroup collapsable="false" name="group">
			<PropertyDaysSelect helpText="${mktMarketingResources.help_time_daysOfTheWeek}" promptText="${mktMarketingResources.daysOfTheWeek}" propertyName="daysOfTheWeekString"/>
			<PropertyTimePicker helpText="${mktMarketingResources.help_time_startTime}" promptText="${mktMarketingResources.startTime}" propertyName="startTime"/>
			<PropertyTimePicker helpText="${mktMarketingResources.help_time_endTime}" promptText="${mktMarketingResources.endTime}" propertyName="endTime"/>
		</PropertyGroup>
	</FlowElementProperties>
</Definitions>