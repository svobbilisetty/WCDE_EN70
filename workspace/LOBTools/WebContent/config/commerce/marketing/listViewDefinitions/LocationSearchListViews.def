<Definitions>
	<!---
		This grid displays the contents and characteristics of a list of {@link LocationPOI LocationPOI}. 
	-->
	<ObjectGrid definitionName="mktSearchLocationPOIGrid" preferenceKey="mktSearchLocationPOIGrid">
		<GridText alignment="left" editable="false" name="regionId"            propertyName="regionId"          text="${mktMarketingResources.regionId}"          visible="false" width="30"  />
		<GridText alignment="left" editable="false" name="pointOfInterestName" propertyName="name"              text="${mktMarketingResources.objectName}"        visible="true"  width="100"  />
		<GridText alignment="left" editable="false" name="description"         propertyName="description"       text="${mktMarketingResources.description}"       visible="true"  width="180" />
	</ObjectGrid>

	<!---
		This grid displays the contents and characteristics of a list of {@link LocationRegion LocationRegion}. 
	-->
	<ObjectGrid definitionName="mktSearchLocationRegionGrid" preferenceKey="mktSearchLocationRegionGrid">
		<GridText alignment="left" editable="false" name="regionId"          propertyName="regionId"          text="${mktMarketingResources.regionId}"          visible="false" width="30"  />
		<GridText alignment="left" editable="false" name="pointOfInterestId" propertyName="pointOfInterestId" text="${mktMarketingResources.pointOfInterestId}" visible="false" width="30"  />
		<GridText alignment="left" editable="false" name="regionName"        propertyName="name"              text="${mktMarketingResources.objectName}"        visible="true"  width="100"  />
		<GridText alignment="left" editable="false" name="description"       propertyName="description"       text="${mktMarketingResources.description}"       visible="true"  width="180" />
	</ObjectGrid>

			
</Definitions>