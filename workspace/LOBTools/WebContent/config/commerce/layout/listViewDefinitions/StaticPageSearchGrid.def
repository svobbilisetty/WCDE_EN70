<Definitions>

	<!---
		This definition displays the contents and characteristics of a static page
	-->
    <ObjectGrid definitionName="pglStaticPageSearchGrid" preferenceKey="pglStaticPageSearchGrid">
		<GridIconTypeImage name="typeIcon" propertyName="null" required="true" text="${pglPageLayoutResources.pageType_ColumnHeader}" visible="true" width="60"/>            
        <GridText editable="false" name="pageType" propertyName="pageType" text="${pglPageLayoutResources.pageTypeName_ColumnHeader}" visible="false" width="90"/>
        <GridText editable="false" name="pageName" propertyName="name" text="${pglPageLayoutResources.pageName_ColumnHeader}" visible="true" width="90"/>
    </ObjectGrid>
    
