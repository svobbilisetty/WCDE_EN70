<Definitions>
	
	<!---	
    	This definition displays the details of a list of
    	{@link srchBaseFacetPrimaryObjectDefinition} 
    	{@link mktBaseSearchFacet}
    	facets that will be excluded from the search request for product recommendations.  
	-->
	<ObjectGrid definitionName="srchFacetExclusionGrid" preferenceKey="srchFacetExclusionGrid">
		<GridComboBox name="displayName" 
			propertyName="facetExclusionField" 
			required="true" 
			text="${srchSearchResources.facetDisplayName}" 
			width="130"/>
	</ObjectGrid>
			
</Definitions>
